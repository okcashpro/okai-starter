{
  "name": "@okcashpro/agent",
  "version": "0.1.1",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsup --format esm --dts",
    "start": "tsc && node --loader ts-node/esm src/index.ts",
    "start:service:all": "pm2 start pnpm --name=\"all\" --restart-delay=3000 --max-restarts=10 -- run start:all",
    "stop:service:all": "pm2 stop all"
  },
  "dependencies": {
    "@okcashpro/adapter-postgres": "v0.1.5-alpha.8",
    "@okcashpro/adapter-sqlite": "v0.1.5-alpha.8",
    "@okcashpro/client-auto": "v0.1.5-alpha.8",
    "@okcashpro/client-direct": "v0.1.5-alpha.8",
    "@okcashpro/client-discord": "v0.1.5-alpha.8",
    "@okcashpro/client-telegram": "v0.1.5-alpha.8",
    "@okcashpro/client-twitter": "v0.1.5-alpha.8",
    "@okcashpro/okai": "v0.1.5-alpha.8",
    "@okcashpro/plugin-bootstrap": "v0.1.5-alpha.8",
    "@okcashpro/plugin-image-generation": "v0.1.5-alpha.8",
    "@okcashpro/plugin-node": "v0.1.5-alpha.8",
    "@okcashpro/plugin-solana": "v0.1.5-alpha.8",
    "@okcashpro/plugin-starknet": "v0.1.5-alpha.8",
    "better-sqlite3": "^11.5.0",
    "readline": "^1.3.0",
    "ws": "^8.18.0",
    "yargs": "17.7.2"
  },
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "overrides": {
      "onnxruntime-node": "^1.20.0"
    }
  },
  "devDependencies": {
    "ts-node": "10.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
